{
  "name": "lofty-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "autoprefixer-stylus": "0.14.0",
    "body-parser": "^1.18.3",
    "concurrently": "3.5.1",
    "express": "^4.16.3",
    "firebase": "^4.10.1",
    "prop-types": "^15.6.0",
    "re-base": "3.2.2",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "react-parallax": "^1.8.0",
    "react-pdf": "^3.0.5",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "react-scripts": "1.1.4",
    "react-stripe-elements": "^2.0.1",
    "react-transition-group": "^2.2.1",
    "serve": "^6.5.8",
    "stripe": "^6.7.0",
    "stylus": "0.54.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "watch": "concurrently --names \"webpack, stylus\" --prefix name \"npm run start\" \"npm run styles:watch\"",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "styles": "stylus -u autoprefixer-stylus ./src/candy_machine/css/style.styl -o ./src/candy_machine/css/style.css",
    "styles:watch": "stylus -u autoprefixer-stylus -w ./src/candy_machine/css/style.styl -o ./src/candy_machine/css/style.css"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.3"
  }
}
